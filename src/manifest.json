{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icons/packr_16.png",
    "48": "icons/packr_48.png",
    "128": "icons/packr_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/packr_19.png",
      "38": "icons/packr_38.png"
    }
  }
}
